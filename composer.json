{
    "name": "promet/provus-drupal",
    "description": "Install Provus Drupal kickstarter by Promet Source.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type":"package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "library-enyo/dropzone",
                "version": "5.7.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "v5.7.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jsanahuja/jqueryinstagramfeed",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jsanahuja/jquery.instagramFeed/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.9",
        "drupal/addtoany": "^1.16",
        "drupal/admin_toolbar": "^3.0",
        "drupal/ala": "1.8",
        "drupal/antibot": "^1.5",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_layout_builder": "^2.0",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/bootstrap_site_alert": "^2.0@beta",
        "drupal/calendar_link": "^2.1",
        "drupal/chosen": "^3.0",
        "drupal/config_ignore": "^2.3",
        "drupal/config_merge": "^1.0@RC",
        "drupal/config_provider": "^2.0@RC",
        "drupal/config_snapshot": "^1.0@RC",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/date_recur": "^3.2",
        "drupal/date_recur_modular": "^3.0",
        "drupal/default_content": "^1.0@alpha",
        "drupal/devel": "^4.1",
        "drupal/entity": "^1.2",
        "drupal/environment_indicator": "^4.0",
        "drupal/exclude_node_title": "1.3",
        "drupal/extlink": "^1.6",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.2",
        "drupal/focal_point": "^1.5",
        "drupal/fontawesome": "^2.22",
        "drupal/formtips": "^1.5",
        "drupal/fullcalendar_view": "6.x-dev@dev",
        "drupal/geocoder": "^3.24",
        "drupal/geofield": "^1.36",
        "drupal/geofield_map": "^2.71",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_login": "1.0",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/google_analytics": "^4.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_builder_at": "^2.12",
        "drupal/layout_builder_browser": "^1.3",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_builder_restrictions": "^2.13",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/layout_library": "^1.0@beta",
        "drupal/layout_paragraphs": "^1.0",
        "drupal/linkit": "^6.0@beta",
        "drupal/media_bulk_upload": "^1.0@alpha",
        "drupal/memcache": "^2.3",
        "drupal/menu_block": "1.x-dev",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/office_hours": "^1.5",
        "drupal/pathauto": "^1.8",
        "drupal/pdfpreview": "^1.0",
        "drupal/redirect": "^1.7",
        "drupal/search_api": "^1.22",
        "drupal/search_api_solr": "^4.2",
        "drupal/section_library": "^1.0",
        "drupal/sitemap": "2.x-dev",
        "drupal/smart_trim": "^1.3",
        "drupal/twig_tweak": "^3.1",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_bootstrap": "4.3",
        "drupal/viewsreference": "^1.7",
        "drupal/vocabulary_condition": "^1.0",
        "drupal/webform": "^6.1",
        "drush/drush": "^10",
        "geocoder-php/google-maps-provider": "^4.6",
        "harvesthq/chosen": "^1.8",
        "kporras07/composer-symlinks": "^1.1",
        "oomphinc/composer-installers-extender": "^2.0",
        "rlanvin/php-rrule": "^2.0",
        "vlucas/phpdotenv": "^5.4",
        "webflo/drupal-finder": "^1.2",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "require-dev": {
        "dmore/behat-chrome-extension": "^1.3",
        "drupal/drupal-extension": "^4.1",
        "mglaman/drupal-check": "^1.2",
        "drupal/core-dev": "^9.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Allow hiding configured blocks in layout builder [3163197]": "https://www.drupal.org/files/issues/2021-11-15/layout_builder-hide-block-3163197-15.patch"
            },
            "drupal/auto_entitylabel": {
                "Integration with Layout Builder for custom block types [3065687]": "https://www.drupal.org/files/issues/2022-12-22/auto_entitylabel-layout_builder_component_title-3065687-17.patch"
            },
            "drupal/exclude_node_title": {
                "Deployment erases excluded-node-titles [2974973]": "https://www.drupal.org/files/issues/2018-06-20/2974973-state_api-3.patch"
            },
            "drupal/bootstrap_site_alert": {
                "HTML markup shown as encoded characters [3281928]": "https://git.drupalcode.org/project/bootstrap_site_alert/-/merge_requests/5.diff"
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    }
}
